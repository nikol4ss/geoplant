{
  "name": "server",
  "version": "1.0.1",
  "description": "Node.js backend in TypeScript + Fastify.",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "prisma:migrate": "pnpm prisma migrate dev",
    "prisma:generate": "pnpm prisma generate",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "Nikolas Campos",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^25.1.0",
    "@types/node-cache": "^4.2.5",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "nodemon": "^3.1.11",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.8.1",
    "prisma": "^7.3.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@prisma/client": "^7.3.0",
    "fastify": "^5.7.2",
    "node-cache": "^5.1.2",
    "pino": "^10.3.0"
  }
}
